library(boot)
set.seed(123)

x <- read.csv(file = "eos_bs.csv", header = TRUE)

i<-nrow(x) #count number of rows for resampling 

boot.mean <- function(x,i){
  boot.mean<-mean(x[i])
}

apply(x,2,function(y){ 
  b_low<-boot(y,boot.mean,R=1001); 
  c(length(which(b_low$t > 149)))
})

apply(x,2,function(y){ 
  b_high<-boot(y,boot.mean,R=1001); 
  c(length(which(b_high$t > 299)))
})
