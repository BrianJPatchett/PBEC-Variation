library(boot)
set.seed(123)

x <- read.csv(file = "eos_bs.csv", header = TRUE)

fc <- function(x, indices) {
  return( mean( x[indices] ) )
}


# I gave up on trying to use a for loop, hence:
x_1 <- na.omit(x$x_1)
bs_1 <- boot(x_1, fc, R=1000, stype = "i")
boot.ci(bs_1)
count_1 <- length(which(bs_1$t > 299))

x_2 <- na.omit(x$x_2)
